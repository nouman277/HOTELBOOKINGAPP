<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FifthScreen"
    android:orientation="vertical">




    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <EditText
            android:layout_width="193dp"
            android:layout_height="40dp"
            android:textColor="#0D0D0D"
            android:background="#FFFFFF"
            android:fontFamily="@font/gothamrounded_bold"
            android:layout_gravity="center"
            android:text="Rooms and Guests"
            android:layout_marginTop="8dp"
            android:textSize="20sp"/>

        <ImageView
            android:layout_width="25dp"
            android:layout_height="30dp"
            android:src="@drawable/group_587"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="30dp"
            android:id="@+id/backbtnFIS"
            />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginTop="35dp">

        <androidx.cardview.widget.CardView
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:cardCornerRadius="40dp"
            android:layout_gravity="left"
            android:layout_marginTop="40dp"
            android:layout_marginLeft="35dp">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/group_3876"/>

        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:cardCornerRadius="40dp"
            android:layout_gravity="right"
            android:layout_marginTop="40dp"
            android:layout_marginRight="35dp">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/group_607"/>

        </androidx.cardview.widget.CardView>

        <TextView
            android:layout_width="77dp"
            android:layout_height="30dp"
            android:layout_gravity="center_horizontal"
            android:text="Rooms"
            android:textSize="23dp"
            android:fontFamily="@font/roboto_regular"
            android:textColor="#0D0D0D"
            android:layout_marginLeft="8dp"/>

        <TextView
            android:layout_width="70dp"
            android:layout_height="105dp"
            android:layout_gravity="center_horizontal"
            android:text="1"
            android:textSize="65dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/gothamroundedmedium_21022"
            android:textColor="#EF4E22"
            />


    </androidx.cardview.widget.CardView>



    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginTop="35dp">

        <androidx.cardview.widget.CardView
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:cardCornerRadius="40dp"
            android:layout_gravity="left"
            android:layout_marginTop="40dp"
            android:layout_marginLeft="35dp">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/group_3876"/>

        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:cardCornerRadius="40dp"
            android:layout_gravity="right"
            android:layout_marginTop="40dp"
            android:layout_marginRight="35dp">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/group_607"/>

        </androidx.cardview.widget.CardView>

        <TextView
            android:layout_width="77dp"
            android:layout_height="30dp"
            android:layout_gravity="center_horizontal"
            android:text="Adults"
            android:textSize="23dp"
            android:fontFamily="@font/roboto_regular"
            android:textColor="#0D0D0D"
            android:layout_marginLeft="14dp"/>

        <TextView
            android:layout_width="70dp"
            android:layout_height="105dp"
            android:layout_gravity="center_horizontal"
            android:text="2"
            android:textSize="65dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/gothamroundedmedium_21022"
            android:textColor="#EF4E22"
            />


    </androidx.cardview.widget.CardView>





    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginTop="35dp">

        <androidx.cardview.widget.CardView
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:cardCornerRadius="40dp"
            android:layout_gravity="left"
            android:layout_marginTop="40dp"
            android:layout_marginLeft="35dp">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/group_3876"/>

        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:cardCornerRadius="40dp"
            android:layout_gravity="right"
            android:layout_marginTop="40dp"
            android:layout_marginRight="35dp">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/group_607"/>

        </androidx.cardview.widget.CardView>

        <TextView
            android:layout_width="89dp"
            android:layout_height="30dp"
            android:layout_gravity="center_horizontal"
            android:text="Children"
            android:textSize="23dp"
            android:fontFamily="@font/roboto_regular"
            android:textColor="#0D0D0D"
            android:layout_marginLeft="14dp"/>

        <TextView
            android:layout_width="70dp"
            android:layout_height="105dp"
            android:layout_gravity="center_horizontal"
            android:text="0"
            android:textSize="65dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/gothamroundedmedium_21022"
            android:textColor="#EF4E22"
            />


    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="340dp"
        android:layout_height="60dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="170dp"
        app:cardBackgroundColor="#EF4E22"
        android:id="@+id/confirmFiS"
        app:cardCornerRadius="15dp"
        app:cardElevation="8dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="CONFIRM"

            android:fontFamily="@font/gothamrounded_book_21018"
            android:textSize="30sp"
            android:layout_gravity="center"
            android:textColor="@color/white"/>



    </androidx.cardview.widget.CardView>





















</LinearLayout>